config EXAMPLES_PRIME
	tristate "Multicore prime number calculator"
	depends on ASMP
	default n
	---help---
		Enable the ELF loader example

if EXAMPLES_PRIME

config EXAMPLES_PRIME_WORKER_STACKSIZE
	int "Example worker stack size"
	default 1024

endif
